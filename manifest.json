{
    "manifest_version": 3,
    "author": "Miguel Zepda",
    "name": "PomoNotes",
    "version": "0.1",
    "description": "This extension overrides the default new tab.",
    "permissions": [
      "storage"
    ], 
    "icons": {
      "128": "assets/icon.png"
    },
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    }
  }